global:
  scrape_interval: 15s

scrape_configs:
  - job_name: 'prometheus'
    static_configs:
      - targets: ['localhost:9090']

  - job_name: 'jenkins'
    static_configs:
      - targets: ['jenkins-master:8080']

  - job_name: 'node-exporter'
    static_configs:
      - targets: ['{{ ansible_host }}:9100']
